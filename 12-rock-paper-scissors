public Integer rockPaperScissors(String player1, String player2) {
    //code here
    if((player1.isEquals('rock') && player2.isEquals('scissors')) ||
       (player1.isEquals('scissors')  && player2.isEquals('paper')) ||
       (player1.isEquals('paper')  && player2.isEquals('rock'))){
        return 1;
    }else if(player1.isEquals(player2)){
        return 0;
    }
    return 2;
}