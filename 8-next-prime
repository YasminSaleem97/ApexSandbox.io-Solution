public Integer nextPrime(Integer num) {
    //code here
    Integer nextNum = num+1;
    while(true){
        if(isPrime(nextNum)){
            return nextNum;
        }
        nextNum++;
    }
}

   public Boolean isPrime(Integer n){
       if(n<2) return false;
       else{
           for(Integer i=2;i<= Math.sqrt(n); i++){
               if(Math.mod(n,i)==0){
                   return false;
               }
           }
           return true;
       }
   
}